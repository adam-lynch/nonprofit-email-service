{
  "name": "nonprofit-email-service",
  "version": "1.0.0",
  "description": "A cost effective, open source solution to sending mass emails",
  "main": "index.js",
  "engines": {
    "npm": ">=3"
  },
  "scripts": {
    "client:start": "npm-run-all --parallel client:test:watch client:open:src client:lint:watch",
    "client:open:src": "babel-node tools/srcServer.js",
    "client:open:dist": "babel-node tools/distServer.js",
    "client:lint": "esw webpack.config.* client tools --color",
    "client:lint:watch": "npm run client:lint -- --watch",
    "client:clean-dist": "npm run client:remove-dist && mkdir dist",
    "client:remove-dist": "rimraf ./dist",
    "client:build": "babel-node tools/build.js && npm run client:open:dist",
    "client:test": "mocha tools/testSetup.js \"client/**/*.spec.js\" --reporter progress",
    "client:test:cover": "babel-node node_modules/isparta/bin/isparta cover --root client--report html node_modules/mocha/bin/_mocha -- --require ./tools/testSetup.js \"client/**/*.spec.js\" --reporter progress",
    "client:test:cover:travis": "babel-node node_modules/isparta/bin/isparta cover --root client--report lcovonly _mocha -- --require ./tools/testSetup.js \"client/**/*.spec.js\" && cat ./coverage/lcov.info | node_modules/coveralls/bin/coveralls.js",
    "client:test:watch": "npm run client:test -- --watch",
    "client:open:cover": "npm run test:cover && open coverage/index.html",
    "start": "node server/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "babel-node tools/buildServer.js",
    "email:server": "node server/email/test-internal",
    "email:test:multiple": "RUN_ALONE=true node server/email/test-internal/test-multiple.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/FreeCodeCamp/nonprofit-email-service.git"
  },
  "author": "FreeCodeCamp",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/FreeCodeCamp/nonprofit-email-service/issues"
  },
  "homepage": "https://github.com/FreeCodeCamp/nonprofit-email-service#readme",
  "dependencies": {
    "admin-lte": "^2.3.6",
    "async": "^2.0.1",
    "body-parser": "^1.15.2",
    "bootstrap": "^3.3.7",
    "csv": "^1.1.0",
    "dotenv": "^2.0.0",
    "express": "^4.14.0",
    "express-session": "^1.14.1",
    "import": "0.0.6",
    "jQuery": "^1.7.4",
    "jquery": "^3.1.0",
    "json-loader": "^0.5.4",
    "lodash": "^4.15.0",
    "nodemailer": "^2.6.0",
    "nodemailer-ses-transport": "^1.4.0",
    "object-assign": "4.1.0",
    "papaparse": "^4.1.2",
    "passport": "^0.3.2",
    "passport-google-oauth": "^1.0.0",
    "pg": "^6.1.0",
    "pg-hstore": "^2.3.2",
    "react": "15.3.1",
    "react-bootstrap": "^0.30.3",
    "react-dom": "15.3.1",
    "react-redux": "4.4.5",
    "react-router": "2.7.0",
    "react-router-redux": "4.0.5",
    "redux": "3.5.2",
    "redux-thunk": "2.1.0",
    "sequelize": "^3.24.3"
  },
  "devDependencies": {
    "autoprefixer": "6.4.0",
    "axios": "^0.14.0",
    "babel-cli": "6.14.0",
    "babel-core": "6.14.0",
    "babel-eslint": "^6.1.2",
    "babel-loader": "6.2.5",
    "babel-plugin-react-display-name": "2.0.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-react-constant-elements": "6.9.1",
    "babel-plugin-transform-react-remove-prop-types": "0.2.9",
    "babel-preset-latest": "6.14.0",
    "babel-preset-react": "6.11.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.13.0",
    "babel-register": "6.14.0",
    "browser-sync": "2.14.0",
    "chai": "3.5.0",
    "chalk": "1.1.3",
    "connect-history-api-fallback": "1.3.0",
    "coveralls": "2.11.12",
    "cross-env": "2.0.1",
    "css-loader": "0.24.0",
    "enzyme": "2.4.1",
    "eslint": "3.4.0",
    "eslint-plugin-import": "1.14.0",
    "eslint-plugin-jsx-a11y": "2.2.0",
    "eslint-plugin-react": "6.2.0",
    "eslint-watch": "2.1.14",
    "extract-text-webpack-plugin": "1.0.1",
    "file-loader": "0.9.0",
    "html-to-react": "^1.2.0",
    "html-webpack-plugin": "2.22.0",
    "isparta": "4.0.0",
    "kue": "^0.11.1",
    "mocha": "3.0.2",
    "mockdate": "1.0.4",
    "nock": "^8.0.0",
    "node-sass": "3.8.0",
    "nodemailer-smtp-transport": "^2.7.2",
    "npm-run-all": "3.0.0",
    "open": "0.0.5",
    "postcss-loader": "0.11.0",
    "prompt": "1.0.0",
    "react-addons-test-utils": "15.3.1",
    "react-fontawesome": "^1.2.0",
    "redux-immutable-state-invariant": "1.2.3",
    "replace": "0.3.0",
    "rimraf": "2.5.4",
    "sass-loader": "4.0.0",
    "sequelize-cli": "^2.4.0",
    "sinon": "1.17.5",
    "sinon-chai": "2.8.0",
    "smtp-server": "^1.14.2",
    "style-loader": "0.13.1",
    "url-loader": "0.5.7",
    "webpack": "1.13.2",
    "webpack-dev-middleware": "1.6.1",
    "webpack-hot-middleware": "2.12.2",
    "webpack-md5-hash": "0.0.5"
  }
}
